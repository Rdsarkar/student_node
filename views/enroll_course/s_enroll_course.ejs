<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Coaching Management System</title>
    <link rel="stylesheet" href="abc/fontawesome/css/all.css" />
    <link rel="stylesheet" href="abc/css/bootstrap.css" />
    <link rel="stylesheet" href="abc/css/style.css" />
    <link rel="stylesheet" href="abc/css/responsive.css" />
</head>

<body>
    <section class="nav-section">
        <header class="admin-panel">
            <nav class="nav-design">
                <div class="logo">
                    <h2>Vanila</h2>
                </div>
                <ul>
                    <li>
                        <a href="" class="btn-primary"><i class="fas fa-bell"></i></a>
                    </li>
                    <li>
                        <a href="/logout" class="n-btn"> Logout </a>
                    </li>
                </ul>
            </nav>
        </header>
    </section>

    <section>
        <div id="wrapper">
                 <!-- Sidebar -->
                 <div class="sidebar">
                    <ul>
                        <li>
                            <i class="fas fa-users"></i> <a href="/home"> Std Profile</a>
                        </li>
                        <li>
                            <i class="fas fa-users"></i> <a href="/enroll_course" class="ad-active">Enroll Course</a>
                        </li>
                        <li>
                            <i class="fas fa-users"></i> <a href="/view_course">View Course</a>
                        </li>
                        <!-- <li>
                            <i class="fas fa-users"></i> <a href="/notes">Notes Download</a>
                        </li> -->
                        <li>
                            <i class="fas fa-users"></i> <a href="/notice">View Notice</a>
                        </li>
                        <li>
                            <i class="fas fa-users"></i> <a href="/result">Check Results</a>
                        </li>
                        <li>
                            <i class="fas fa-users"></i> <a href="/msg">Send Message</a>
                        </li>
                        <!-- <li>
                            <i class="fas fa-users"></i> <a href="s_payment.html">Payment</a>
                        </li>
                        <li>
                            <i class="fas fa-users"></i> <a href="s_pre_course.html">Pre. Course</a>
                        </li> -->
    
                    </ul>
                </div>
                <!-- Sidebar -->

            <div id="content-wrapper">
                <div class="container-fluid">
                    <div class="row justify-content-center">
                        <div class="col-lg-6">
                            <h1 class="mt-5">Enroll Courses</h1>
                            <form class="form" action="" method="post">
								
                                <div class="form-group mb-3 mt-3">
                                    <label for="SelectCourse" class="font-weight-bold">Select Course Type</label>
                                <select class="form-control" name="course_type">
                                    

                                    <% for(var i=0; i < results.length; i++){%>
                                        
                                    <option> <%=results[i].ctype%></option>
                                    <% }%>
                                </select>
                                </div>
                                
                               
                                <div class="form-group">
                                    <label for="SelectBatch" class="font-weight-bold">Batch</label>
                                    <select class="form-control" name="batch">
                                        <% for(var i=0; i < results.length; i++){%>
                                        
                                            <option> <%=results[i].batch%></option>
                                            <% }%>
                                    </select>
                                
                                </div>

                                

                                <div class="form-group">
                                    <label for="SelectBatch" class="font-weight-bold">Subject</label>
                                    <select class="form-control" name="subject">
                                        <% for(var i=0; i < results.length; i++){%>
                                        
                                            <option> <%=results[i].subject%></option>
                                            <% }%>
                                    </select>
                                
                                </div>    

                                <div class="form-group">
                                    <label for="SelectBatch" class="font-weight-bold">Payment</label>
                                    <select class="form-control" name="payment">
                                        <% for(var i=0; i < results.length; i++){%>
                                        
                                            <option> <%=results[i].payment%></option>
                                            <% }%>
                                    </select>
                                
                                </div>

                                <div class="form-group">
                                    <label for="SelectBatch" class="font-weight-bold">Class Time</label>
                                    <select class="form-control" name="time">
                                        <% for(var i=0; i < results.length; i++){%>
                                        
                                            <option> <%=results[i].classtime%></option>
                                            <% }%>
                                    </select>
                                
                                </div>

                                <div class="form-group">
                                    <label for="SelectBatch" class="font-weight-bold">Class Day</label>
                                    <select class="form-control" name="day">
                                        <% for(var i=0; i < results.length; i++){%>
                                        
                                            <option> <%=results[i].classday%></option>
                                            <% }%>
                                    </select>
                                
                                </div>
                              
                                <input type="submit" name="submit" value="Submit" class="btn btn-success" id="submit">
                            </form> 
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="abc/js/jquery.js"></script>
    <script src="abc/js/proper.js"></script>
    <script src="abc/js/bootstrap.js"></script>
</body>

</html>